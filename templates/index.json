    "kawa-choose-reason": {
      "type": "kawa-choose-reason",
      "blocks": {
        "card-truck": {
          "type": "reason_card",
          "settings": {
            "card_title": "Fast, Free Shipping",
            "card_content": "<p>We offer free shipping straight to your door throughout North America, Europe, and the Middle East*.<\/p><p><\/p><p>It usually takes 3-7 business days* for your package to be delivered, so you can sit back and relax, worry-free. Except for some special countries.<\/p><p>*Please see our shipping policy for more details.<\/p><p>*Due to COVID-19 shipping times may vary.<\/p>",
            "card_icon": "icon-truck"
          }
        },
        "card-return": {
          "type": "reason_card",
          "settings": {
            "card_title": "30-Day Free and Easy Returns",
            "card_content": "<p>Get a full refund when you return your undamaged products within 30days - for any reason. Once the item arrives back at the KAWA warehouse for inspection, the refund process will begin.<\/p><p>See our refund policy for more details about what is needed for a smooth refund process..<\/p>",
            "card_icon": "icon-return"
          }
        },
        "card-warranty": {
          "type": "reason_card",
          "settings": {
            "card_title": "365 Days Warranty",
            "card_content": "<p>KAWA provides warranty service and official customer support for 1 year. If you’re not totally satisfied with the products or request a repair, please feel free to find on the product listing or contact us at support@kawa-in.com with your order receipt for confirmation.<\/p>",
            "card_icon": "icon-warranty"
          }
        }
      },
      "block_order": [
        "card-truck",
        "card-return",
        "card-warranty"
      ],
